{
  "name": "pollen-solutions/wp-app",
  "description": "Pollen Wordpress App - Micro Framework for Wordpress App.",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-app/",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "micro framework"
  ],
  "time": "2021-01-21",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.3",
    "composer/installers": "~1.0",
    "illuminate/support": "^8.22",
    "laminas/laminas-diactoros": "^2.5",
    "laminas/laminas-httphandlerrunner": "^1.3",
    "league/container": "^3.3",
    "league/plates": "^3.4",
    "league/route": "^4.5",
    "symfony/http-foundation": "^5.2",
    "symfony/psr-http-message-bridge": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpApp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpApp\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
